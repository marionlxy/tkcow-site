<?xml version="1.0" encoding="UTF-8" ?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">
    
  <description>JSTL 1.1 core library</description>
  <display-name>JSTL core</display-name>
  <tlib-version>1.1</tlib-version>
  <short-name>xsh</short-name>
  <uri>http://java.sun.com/jsp/jstl/pagess</uri>
  
  <tag>
  	<name>page</name>
  	<tag-class>org.baitanw.maomao.utils.PageTag</tag-class>
  	<body-content>scriptless</body-content>
  	<attribute>
  		<name>formId</name>
  		<required>true</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>leftPages</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>int</type>
  	</attribute>
  	<attribute>
  		<name>rightPages</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>int</type>
  	</attribute>
  	<attribute>
  		<name>pageCount</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>int</type>
  	</attribute>
  	<attribute>
  		<name>firstName</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>lastName</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>prevName</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>nextName</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>showFirst</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>boolean</type>
  	</attribute>
  	<attribute>
  		<name>showLast</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>boolean</type>
  	</attribute>
  	<attribute>
  		<name>showPrev</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>boolean</type>
  	</attribute>
  	<attribute>
  		<name>showNext</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>boolean</type>
  	</attribute>
  	<attribute>
  		<name>targetPage</name>
  		<required>true</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>fieldNames</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>headerNames</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>pageBean</name>
  		<required>true</required>
  		<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>tableClass</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  	<attribute>
  		<name>paggingClass</name>
  		<required>false</required>
  		<rtexprvalue>true</rtexprvalue>
  		<type>java.lang.String</type>
  	</attribute>
  </tag>
</taglib>